{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "camelcase": "off",
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off",
    "consistent-return": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
